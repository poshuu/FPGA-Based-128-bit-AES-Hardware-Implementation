S-Box C Script 
/* Open-Source implementation of S-box from Wikipedia page on S-box creation. 
*/  
#include <stdio.h>  
#include <stdint.h>  
#define ROTL8(x,shift) ((uint8_t) ((x) << (shift)) | ((x) >> (8 - (shift))))  
void initialize_aes_sbox(uint8_t sbox[256]) {  
uint8_t p = 1, q = 1;  
int i;  
/* loop invariant: p * q == 1 in the Galois field */  
do {  
/* multiply p by 3 */  
p = p ^ (p << 1) ^ (p & 0x80 ? 0x1B : 0);  
/* divide q by 3 (equals multiplication by 0xf6) */  
q ^= q << 1;  
q ^= q << 2;  
q ^= q << 4;  
q ^= q & 0x80 ? 0x09 : 0;  
/* compute the affine transformation */  
uint8_t xformed = q ^ ROTL8(q, 1) ^ ROTL8(q, 2) ^ ROTL8(q, 3) ^ ROTL8(q, 4);  
sbox[p] = xformed ^ 0x63;  
} while (p != 1);
/* 0 is a special case since it has no inverse */  
sbox[0] = 0x63;  
} 
/* Generates list to check with Vivado implementation of S-box. */  
void print_sbox() {  
unsigned char aes_sbox[256];  
initialize_aes_sbox(aes_sbox);  
printf("Starting exhaustive listing of AES S-Box...\n");  
printf("In | Out\n");  
for (int i = 0; i < 256; i++){  
printf("%3d | 0x%02x\n", i, aes_sbox[i]);  
}  
printf("Test completed.");  
}  
int main() {  
print_sbox();  
}  
